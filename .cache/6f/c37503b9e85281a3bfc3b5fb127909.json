ine":105,"column":41}},{"source":"src/view/View.ts","name":null,"original":{"line":55,"column":4},"generated":{"line":105,"column":48}},{"source":"src/view/View.ts","name":null,"original":{"line":56,"column":3},"generated":{"line":106,"column":0}},{"source":"src/view/View.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":112,"column":0}},{"source":"src/view/View.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":112,"column":7}},{"source":"src/view/View.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":112,"column":8}},{"source":"src/view/View.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":112,"column":12}},{"source":"src/view/View.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":112,"column":15}},{"source":"src/view/View.ts","name":null,"original":{"line":3,"column":0},"generated":{"line":112,"column":19}}],"sources":{"src/view/View.ts":"import { Model } from '../model/Model';\n\nexport abstract class View<T extends Model<K>, K> {\n  regions: { [key: string]: Element } = {};\n\n  constructor(public parent: Element, public model: T) {\n    this.bindModel();\n  }\n\n  bindModel(): void {\n    this.model.on('change', () => {\n      this.render();\n    });\n  }\n  bindEvents(fragment: DocumentFragment): void {\n    const eventsMap = this.eventsMap();\n    for (let eventKey in eventsMap) {\n      const [eventName, selector] = eventKey.split(':');\n      fragment.querySelectorAll(selector).forEach((element: Element) => {\n        element.addEventListener(eventName, eventsMap[eventKey]);\n      });\n    }\n  }\n  // abstract eventsMap(): { [key: string]: () => void }; //we dont need to compalsuryly implement it in any kind of userform so commenting it from abstract//optional method\n  regionsMap(): { [key: string]: string } {\n    return {};\n  }\n  eventsMap(): { [key: string]: () => void } {\n    return {};\n  }\n  abstract template(): string;\n\n  //helper method for render method\n  mapRegions(fragment: DocumentFragment): void {\n    const regionsMap = this.regionsMap();\n    for (let key in regionsMap) {\n      const selector = regionsMap[key];\n      const element = fragment.querySelector(selector);\n      if (element) {\n        this.regions[key] = element;\n      }\n    }\n  }\n  //helper method for render method\n  onRender(): void {}\n  render(): void {\n    this.parent.innerHTML = '';\n    const templateElement = document.createElement('template');\n    templateElement.innerHTML = this.template();\n    this.bindEvents(templateElement.content); //content=>document fragment\n    this.mapRegions(templateElement.content);\n\n    this.onRender();\n\n    this.parent.append(templateElement.content);\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"f5abf72916cad9d3c0a740e63bb2c58d","cacheData":{"env":{}}}